  "type": "module",
  "scripts": {
    "sdkupdate": "yarn dlx @yarnpkg/sdks vscode",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
